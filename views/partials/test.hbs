<tr data-id='{{ id }}'>
	<td>{{ name }}</td>
	<td>{{ class }}</td>
	<td>{{ subject }}</td>
	<td>{{ date }}</td>
</tr>

<script>
	document.querySelector('.tests tbody tr[data-id="{{id}}"]').addEventListener(touchEvent, (e) => {
		const path = e.path || (e.composedPath && e.composedPath());
		const test = path[1];
		window.location.href = `/admin/test/${test.getAttribute('data-id')}`;
	});
</script>
